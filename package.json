{
  "name": "astradb-datasource",
  "version": "0.6.3",
  "description": "Grafana Data Source Backend Plugin for Astra DB",
  "private": true,
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e:playwright": "yarn playwright test",
    "e2e:playwright:ui": "yarn playwright test --ui",
    "e2e:playwright:report": "yarn playwright show-report",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier:write": "prettier --list-different \"**/*.{js,ts,tsx,scss,md,mdx}\" --write",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "10.2.2",
    "@grafana/experimental": "1.7.10",
    "@grafana/plugin-ui": "0.7.2",
    "@grafana/runtime": "10.2.2",
    "@grafana/ui": "10.4.1",
    "@playwright/test": "^1.47.2",
    "dotenv": "^16.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-virtualized-auto-sizer": "^1.0.24",
    "sql-formatter-plus": "^1.3.6",
    "tslib": "2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e-selectors": "10.4.1",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/plugin-e2e": "0.18.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react": "17.0.22",
    "@types/testing-library__jest-dom": "^5.9.5",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^8.15.2",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "rxjs": "7.3.0",
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.19"
}
