{
  "name": "astradb-datasource",
  "version": "1.0.3",
  "description": "Grafana Data Source Backend Plugin for Astra DB",
  "private": true,
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e:playwright": "yarn playwright test",
    "e2e:playwright:ui": "yarn playwright test --ui",
    "e2e:playwright:report": "yarn playwright show-report",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "prettier:write": "prettier --list-different \"**/*.{js,ts,tsx,scss,md,mdx}\" --write",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "e2e": "playwright test"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^11.5.2",
    "@grafana/plugin-ui": "^0.10.1",
    "@grafana/runtime": "^11.5.2",
    "@grafana/schema": "^11.5.2",
    "@grafana/ui": "^11.5.2",
    "dotenv": "^16.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "sql-formatter-plus": "^1.3.6",
    "tslib": "2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.17.1",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "1.50.1",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react": "17.0.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "^8.18.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^61.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.85.0",
    "sass-loader": "16.0.5",
    "semver": "^7.6.3",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.2.0-rc.1",
    "webpack-virtual-modules": "^0.6.2",
    "@types/testing-library__jest-dom": "5.14.9"
  },
  "resolutions": {
    "rxjs": "7.3.0",
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.19"
}
