{
  "name": "astradb-datasource",
  "version": "0.3.0",
  "description": "Grafana Data Source Backend Plugin for Astra DB",
  "private": true,
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "e2e": "grafana-e2e run",
    "e2e:open": "grafana-e2e open",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch",
    "prettier:write": "prettier --list-different \"**/*.{js,ts,tsx,scss,md,mdx}\" --write"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@grafana/data": "8.4.7",
    "@grafana/experimental": "^0.0.2-canary.39",
    "@grafana/plugin-ui": "^0.1.11",
    "@grafana/runtime": "8.4.7",
    "@grafana/ui": "8.4.7",
    "react-virtualized-auto-sizer": "^1.0.6",
    "sql-formatter-plus": "^1.3.6"
  },
  "devDependencies": {
    "@grafana/e2e": "8.4.7",
    "@grafana/e2e-selectors": "8.4.7",
    "@grafana/toolkit": "8.4.7",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/lodash": "^4.14.184",
    "@types/react": "17.0.22",
    "@types/testing-library__jest-dom": "^5.9.5",
    "cspell": "^6.8.1",
    "cypress": "7.7.0"
  },
  "resolutions": {
    "rxjs": "7.3.0"
  }
}
